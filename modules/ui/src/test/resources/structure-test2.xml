        <tocItem>
            <aknTag>point</aknTag>
            <root>false</root>
            <draggable>false</draggable>
            <childrenAllowed>true</childrenAllowed>
            <display>false</display>
            <itemNumber>mandatory</itemNumber>
            <itemHeading>none</itemHeading>
            <itemDescription>false</itemDescription>
            <numberEditable>false</numberEditable>
            <contentDisplayed>true</contentDisplayed>
            <deletable>true</deletable>
            <numWithType>false</numWithType>
            <expandedByDefault>true</expandedByDefault>
            <sameParentAsChild>true</sameParentAsChild>
            <numberingType>MULTILEVEL</numberingType>
            <editable>false</editable>
        </tocItem>

        <tocItem>
            <aknTag>alinea</aknTag>
            <root>false</root>
            <draggable>false</draggable>
            <childrenAllowed>false</childrenAllowed>
            <display>false</display>
            <itemNumber>none</itemNumber>
            <itemHeading>none</itemHeading>
            <itemDescription>false</itemDescription>
            <numberEditable>false</numberEditable>
            <contentDisplayed>true</contentDisplayed>
            <deletable>true</deletable>
            <numWithType>false</numWithType>
            <expandedByDefault>true</expandedByDefault>
            <sameParentAsChild>false</sameParentAsChild>
            <numberingType>NONE</numberingType>
            <editable>false</editable>
        </tocItem>
    </tocItems>

    <numberingConfigs>
        <numberingConfig>
            <type>ARABIC</type>
            <prefix></prefix>
            <suffix></suffix>
            <sequence>1</sequence>
            <description>1, 2, etc</description>
            <regex>\d+$|#</regex>
            <msgValidationError>arabic.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ARABIC-POSTFIX</type>
            <prefix></prefix>
            <suffix>.</suffix>
            <sequence>1</sequence>
            <description>1., 2., etc</description>
            <regex>\d+$|#</regex>
            <msgValidationError>arabic.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ARABIC-PARENTHESIS</type>
            <prefix>(</prefix>
            <suffix>)</suffix>
            <sequence>1</sequence>
            <description>(1), (2), etc</description>
            <regex>\d+$|#</regex>
            <msgValidationError>arabic.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ALPHA</type>
            <prefix>(</prefix>
            <suffix>)</suffix>
            <sequence>a</sequence>
            <description>(a), (b), etc</description>
            <regex></regex>
            <msgValidationError></msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ROMAN-LOWER</type>
            <prefix>(</prefix>
            <suffix>)</suffix>
            <sequence>i</sequence>
            <description>(i), (ii), etc</description>
            <regex></regex>
            <msgValidationError></msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ROMAN-UPPER</type>
            <prefix></prefix>
            <suffix></suffix>
            <sequence>I</sequence>
            <description>I, II, etc</description>
            <regex>M{0,4}(CM|CD|D?C{0,3})(XC|XL|L?X{0,3})(IX|IV|V?I{0,3})$|#</regex>
            <msgValidationError>roman.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>INDENT</type>
            <prefix></prefix>
            <suffix></suffix>
            <sequence>-</sequence>
            <description>-, -, etc for all points</description>
            <regex></regex>
            <msgValidationError></msgValidationError>
        </numberingConfig>

        <numberingConfig>
            <type>NONE</type>
        </numberingConfig>

        <numberingConfig>
            <type>MULTILEVEL</type>
            <levels>
                <level>
                    <depth>1</depth>
                    <numberingType>ALPHA</numberingType>
                </level>
                <level>
                    <depth>2</depth>
                    <numberingType>ARABIC-PARENTHESIS</numberingType>
                </level>
                <level>
                    <depth>3</depth>
                    <numberingType>ROMAN-LOWER</numberingType>
                </level>
                <level>
                    <depth>4</depth>
                    <numberingType>INDENT</numberingType>
                </level>
            </levels>
        </numberingConfig>
    </numberingConfigs>

    <tocRules>
        <entry>
            <tocItem>preface</tocItem>
            <list/>
        </entry>
        <entry>
            <tocItem>mainBody</tocItem>
